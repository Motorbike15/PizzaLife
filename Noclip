local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")

local noclipEnabled = false

local function toggleNoclip()
    noclipEnabled = not noclipEnabled
    rootPart.CanCollide = not noclipEnabled -- Simplified collision toggle
    print("Noclip toggled:", noclipEnabled)
end

local function moveForward()
    if noclipEnabled then
        rootPart.CFrame = rootPart.CFrame * CFrame.new(0, 0, -5) -- Move 5 studs forward
    end
end

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessedEvent)
    if input.KeyCode == Enum.KeyCode.E and not gameProcessedEvent then
        toggleNoclip()
    end
    if input.KeyCode == Enum.KeyCode.W and not gameProcessedEvent then
        moveForward()
    end
end)
